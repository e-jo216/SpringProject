<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<!-- 충돌없이 사용하기 위해 definition name은 앞에 member나 board 등 unique한 이름을 붙이면서 작명해주세요 -->
<!-- xml 파일의 이름을 변경하실 경우, servlet-context.xml와 root-context.xml에서도 변경해주세요 -->

<tiles-definitions>
    <definition name="questionList" extends="main">
    	<put-attribute name="title" value="문의 목록"/>
    	<put-attribute name="body" value="/WEB-INF/views/question/questionList.jsp"/>
    </definition>
    <definition name="questionRegister" extends="main">
    	<put-attribute name="title" value="문의 등록"/>
    	<put-attribute name="body" value="/WEB-INF/views/question/questionRegister.jsp"/>
    </definition>
    <definition name="questionView" extends="main">
    	<put-attribute name="title" value="문의 상세 페이지"/>
    	<put-attribute name="body" value="/WEB-INF/views/question/questionView.jsp"/>
    </definition>
    <definition name="questionModify" extends="main">
    	<put-attribute name="title" value="문의 수정"/>
    	<put-attribute name="body" value="/WEB-INF/views/question/questionModify.jsp"/>
    </definition>
    <definition name="qCheckPass" extends="main">
    	<put-attribute name="title" value="비밀번호 확인"/>
    	<put-attribute name="body" value="/WEB-INF/views/question/qCheckPass.jsp"/>
    </definition>
    
</tiles-definitions>



